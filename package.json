{
  "name": "turf-booking-api",
  "version": "2.0.0",
  "description": "Turf booking system with TypeORM",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/db/data.source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/db/data.source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/db/data.source.ts"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.2",
    "@types/pg": "^8.15.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}